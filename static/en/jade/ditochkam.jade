									li
										a(href="/#our-contacts") Contacts

							.main-menu-toggle
								.one
								.two
								.three
			
			// Slider 
			section#slider
				.container-fluid
					.row
						.col-md-12
							.page-title
								h1
									span.letter-1 F
									span.letter-2 o
									span.letter-3 r
									span.letter-4 
									span.letter-4 C
									span.letter-5 h
									span.letter-6 i
									span.letter-7 l
									span.letter-8 d
									span.letter-7 r
									span.letter-3 e
									span.letter-4 n
					.row
						.col-md-12
							.book-slider.owl-carousel.owl-theme
								.item
									img.hidden-xs(src="../img/slide-1-en.png", alt="alt")
									img.visible-xs(src="../img/slide-1-mobile.png", alt="alt")
									a(href="multiomega.html").circle.circle-1
										img(src="../img/round-1-en.png", alt="alt")
								.item
									img.hidden-xs(src="../img/slide-2-en.png", alt="alt")
									img.visible-xs(src="../img/slide-2-mobile.png", alt="alt")
									a(href="vitamine-c.html").circle.circle-2
										img(src="../img/round-2-en.png", alt="alt")
									a(href="ukachivanie.html").circle.circle-3
										img(src="../img/round-3-en.png", alt="alt")
								.item
									.girl
										img(src="../img/girl.png", alt="alt")
									img.hidden-xs(src="../img/slide-3-en.png", alt="alt")
									img.visible-xs(src="../img/slide-3-mobile.png", alt="alt")
									a(href="multivitamine.html").circle.circle-4
										img(src="../img/round-4-en.png", alt="alt")
								.item
									img.hidden-xs(src="../img/slide-4-en.png", alt="alt")
									img.visible-xs(src="../img/slide-4-mobile.png", alt="alt")

		
